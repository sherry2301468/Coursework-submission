#include <iostream>
#include <vector>
using namespace std;

int weigh(int x, int y, const vector<int>& w) {
    if (w[x] < w[y]) return -1;
    if (w[x] > w[y]) return 1;
    return 0;
}

string determineFakeNature(const vector<int>& w) {
    int A = 0, B = 1, C = 2;

    int first = weigh(A, B, w);

    if (first == 0) {
        int second = weigh(A, C, w);
        if (second == 1) return "Coin C is lighter.";
        if (second == -1) return "Coin C is heavier.";
        return "All three coins have equal weight.";
    }

    int second = weigh(A, C, w);

    if (second == 0) {
        if (first == 1) return "Coin B is lighter.";
        else return "Coin B is heavier.";
    } else {
        if (second == 1) return "Coin A is lighter.";
        else return "Coin A is heavier.";
    }
}

int main() {
    vector<int> coins = {10, 10, 9};
    cout << determineFakeNature(coins) << endl;
    return 0;
}
