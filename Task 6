#include <iostream>
#include <vector>
using namespace std;

int weigh(int x, int y, const vector<int>& w) {
    if (w[x] < w[y]) return -1;
    if (w[x] > w[y]) return 1;
    return 0;
}

string determineFakeNature(const vector<int>& w) {
    int A = 0, B = 1, C = 2;

    int first = weigh(A, B, w);

    if (first == 0) {
        int second = weigh(A, C, w);
        if (second == 1) return "Fake coin is heavier.";
        if (second == -1) return "Fake coin is lighter.";

        for (int i = 3; i < w.size(); i++) {
            int t = weigh(A, i, w);
            if (t == 1) return "Fake coin is heavier.";
            if (t == -1) return "Fake coin is lighter.";
        }

        return "Unable to determine.";
    }

    int second = weigh(A, C, w);

    if (second == 0) {
        if (first == 1) return "Fake coin is lighter.";
        return "Fake coin is heavier.";
    }

    if (first == 1) return "Fake coin is heavier.";
    return "Fake coin is lighter.";
}

int main() {
    int n;
    cout << "Enter number of coins: ";
    cin >> n;

    vector<int> coins(n, 10);

    int fakeIndex = n - 1;
    int type = 0;

    if (type == 0) coins[fakeIndex] = 9;
    else coins[fakeIndex] = 11;

    cout << determineFakeNature(coins) << endl;
    return 0;
}
